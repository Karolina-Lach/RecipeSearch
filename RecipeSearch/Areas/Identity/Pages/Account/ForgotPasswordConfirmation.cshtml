@page
@model ForgotPasswordConfirmation
@{
    ViewData["Title"] = "Forgot password confirmation";
}

<br />
<div class="container">
    <div class="row justify-content-center">
        <h1>@ViewData["Title"]</h1>
    </div>
</div>
<hr class="solid">
<div class="container">
    <div class="row justify-content-center">
        <p>
            Please check your email to reset your password.
        </p>
    </div>
</div>
