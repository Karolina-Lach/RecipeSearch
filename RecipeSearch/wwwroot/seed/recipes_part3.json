    {
      "Name": "Chips and fish",
      "PrepTime": "30",
      "Source": "The Good Eats Cookbook",
      "Servings": "4",
      "NoDairy": "true",
      "NoGluten": "true",
      "Vegan": "false",
      "Vegetarian": "true",
      "Healthy": "false",
      "MealTypesString": "lunch,dinner",
      "CuisinesString": "british",
      "Steps": [
        {
          "Number": "1",
          "Name": "Heat oven to 100C. Heat the safflower oil in a 5-quart Dutch oven over high heat"
        },
        {
          "Number": "2",
          "Name": "Using a V-slicer with a wide blade, slice the potatoes with the skin on. Place in a large bowl with cold water. "
        },
        {
          "Number": "3",
          "Name": "In a bowl, whisk together the flour, baking powder, salt, cayenne pepper, and fish seasoning."
        },
        {
          "Number": "4",
          "Name": "Whisk in the beer until the batter is completely smooth and free of any lumps. Refrigerate for 15 minutes. Note: The batter can be made up to 1 hour ahead of time. "
        },
        {
          "Number": "5",
          "Name": "Drain potatoes thoroughly, removing any excess water. When oil reaches 160°, submerge the potatoes in the oil.  "
        },
        {
          "Number": "6",
          "Name": "Working in small batches, fry for 2 to 3 minutes until they are pale and floppy. "
        },
        {
          "Number": "7",
          "Name": " Remove from oil, drain, and cool to room temperature.  Increase the temperature of the oil to 190°."
        },
        {
          "Number": "8",
          "Name": "Re-immerse fries and cook until crisp and golden brown, about 2 to 3 minutes. Remove and drain on roasting rack."
        },
        {
          "Number": "9",
          "Name": "Season with kosher salt while hot and hold in the oven. Allow oil to return to 180°. "
        },
        {
          "Number": "10",
          "Name": "Lightly dredge fish strips in cornstarch."
        },
        {
          "Number": "11",
          "Name": "Working in small batches, dip the fish into batter and immerse into hot oil."
        },
        {
          "Number": "12",
          "Name": "When the batter is set, turn the pieces of fish over and cook until golden brown, about 2 minutes. "
        },
        {
          "Number": "13",
          "Name": "Drain the fish on the roasting rack."
        }
      ],
