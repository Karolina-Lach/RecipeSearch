    {
      "Name": "Chimney tuna loin",
      "PrepTime": "15",
      "Source": "The Good Eats Cookbook",
      "Servings": "4",
      "NoDairy": "true",
      "NoGluten": "true",
      "Vegan": "false",
      "Vegetarian": "true",
      "Healthy": "true",
      "MealTypesString": "lunch,dinner",
      "CuisinesString": "japanese",
      "Steps": [
        {
          "Number": "1",
          "Name": "In a non-reactive bowl combine soy, honey, and wasabi powder. Reserve 1/4 cup for dipping sauce. "
        },
        {
          "Number": "2",
          "Name": "Roll each piece of tuna in this mixture to coat evenly. "
        },
        {
          "Number": "3",
          "Name": "Marinate from 1 hour to overnight."
        },
        {
          "Number": "4",
          "Name": "Remove the tuna from the marinade and discard the marinade. "
        },
        {
          "Number": "5",
          "Name": "On a plate, lay the sesame seeds. Roll the tuna in the seeds to evenly coat."
        },
        {
          "Number": "6",
          "Name": "Fire up the chimney and top with a well-oiled grate. "
        },
        {
          "Number": "7",
          "Name": "Sear for 15 to 30 seconds per side or to desired temperature."
        },
        {
          "Number": "8",
          "Name": "Remove to rack and rest for 3 minutes. "
        },
        {
          "Number": "9",
          "Name": "Cover with foil or plastic wrap to achieve carry over cooking. "
        },
        {
          "Number": "10",
          "Name": "Slice thinly and serve with the dipping sauce."
        }
